<body>
  <div class='flex-container'>
    <div class="flex-items" id="new_user_button">

      <% if session[:user_id] %>
        <%= button_to 'Log Out', logout_path, method: :get, class: 'button1' %>
      <% else %>
        <%= button_to 'Create a New User', register_path, method: :get, class: 'button1' %>
        <%= button_to 'Log In', login_path, method: :get, class: 'button1' %>
      <% end %>
    </div>

    <div class='flex-container' id="existing_users">
      <h3>Existing Users</h3>
      <% if session[:user_id] %>
        <ul class="demo">
          <% @users.each do |user| %>
            <li><%= "#{user.email}" %></li>
          <% end %>
        </ul>
      <% end %>
    </div>
  </div>
</body>